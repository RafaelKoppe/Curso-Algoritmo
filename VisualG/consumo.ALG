Algoritmo "consumo"


Var
   distancia: real
   combustivel: real
   media: real

Inicio

   escreva("Distancia percorrida: ")
   leia(distancia)
   escreva("Combustível gasto: ")
   leia(combustivel)
   
   media <- distancia / combustivel
   
   escreva("Consumo medio: ", media:5:3)



Fimalgoritmo