Algoritmo "funções matemáticas"


Var

   x, y, z, a, b, c, d : real



Inicio

      x <- 2.0
      y <- 3.0
      z <- -10.0
      
      a <- exp(x,y)
      b <- raizq(y)
      c <- pi
      d <- abs(z)
      
      escreval(a)
      escreval(b)
      escreval(c)
      escreval(d)

Fimalgoritmo